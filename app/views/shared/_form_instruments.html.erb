<%= link_to "Get back to index", instruments_path, class: "btn btn-dark mt-3 mb-3"%>
<%= simple_form_for(instrument) do |f| %>
  <%= f.input :name %>
  <%= f.input :category %>
  <%= f.input :brand %>
  <%= f.input :age %>
  <%= f.input :price %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit class: "btn btn-dark" %>
<% end %>
